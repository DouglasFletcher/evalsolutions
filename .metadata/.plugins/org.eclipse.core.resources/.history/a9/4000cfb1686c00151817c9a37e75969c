package test1;

import static org.junit.Assert.*;

import org.junit.Test;

public class JavaTestTestPlus {

	@Test
	public void testPlus() {
		
		JavaTest javaTest = new JavaTest();

		int result = javaTest.plus(2, 3);
		assertEquals(5, result);
	
		String result2 = javaTest.concatenate("word1", "word2");
		assertEquals(result2, "word1word2");
		
	}

}
